{
    "name": "acss-searcher",
    "description": "A handy Atomic CSS class searcher on Chrome extension",
    "version": "1.0",
    "permissions": ["tabs", "http://*/*", "https://*/*"],
    "browser_action": {
        "default_title": "Set this page's color.",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}
