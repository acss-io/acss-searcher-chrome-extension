{
  "name": "acss-searcher",
  "description": "A handy Atomic CSS class searcher on Chrome extension",
  "version": "0.1.2",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "manifest_version": 2
}